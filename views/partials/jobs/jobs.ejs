<div class="span-3 first">
    <div class="strong"><%= jobs[0].department %></div>
</div>

<div class="span-11 last">
    <ul>
        <% jobs.forEach(function(job) { %>
        <li>
            <a href="<%= job['url'] %>"><%= job.title %></a>
        </li>
        <% }); %>
    </ul>
</div>

 <div class="clear" style="margin-bottom: 20px;"></div>