<div class="content log">
<div class="span-17">
    <% log.history().forEach(function(item) { %>
    <div class="span-17 first last">
        <span class="time"><%= log.ago(item.date) %></span>
        <span class="file"><%= item.file %></span>
        <%
            item.messages.forEach(function(msg) {

                switch (typeof msg) {
                    case 'string':
                %>
                        <span class="string"><%= msg.replace(/\n/g, '<br/>') %></span>
                <%
                        break;
                    case 'number':
                %>
                        <span class="number"><%= msg %></span>
                <%
                        break;
                    case 'function':
                %>
                        <span class="function">[Function]</span>
                <%
                        break;
                default:
                    %>
                    <%- debug(msg) %>
                    <%
                    break;
                }
            });
    %>
    </div>
    <div class="clear"></div>
    <% }); %>
</div>

</div>
